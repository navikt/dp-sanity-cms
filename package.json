{
  "name": "dp-sanity-cms",
  "private": true,
  "version": "1.0.0",
  "description": "CMS/sanity-studio for dagpengerelatert innhold",
  "main": "package.json",
  "author": "NAV",
  "license": "MIT",
  "scripts": {
    "start": "sanity start",
    "test": "sanity check",
    "deploy": "npm run deploySchema && npm run deployStudio",
    "deployStudio": "sanity deploy",
    "deploySchema": "sanity graphql deploy --no-playground"
  },
  "dependencies": {
    "@sanity/base": "^1.149.13",
    "@sanity/block-content-to-react": "^2.0.7",
    "@sanity/components": "^1.149.10",
    "@sanity/core": "^1.149.12",
    "@sanity/dashboard": "^1.149.10",
    "@sanity/default-layout": "^1.149.10",
    "@sanity/default-login": "^1.149.11",
    "@sanity/desk-tool": "^1.149.10",
    "@sanity/vision": "^1.149.0",
    "prop-types": "^15.6",
    "react": "^16.2",
    "react-dom": "^16.2",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@types/styled-components": "^5.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,less,css,md}": [
      "prettier --write"
    ]
  }
}
