{
  "name": "dp-sanity-cms",
  "private": true,
  "version": "1.0.0",
  "description": "CMS/sanity-studio for dagpengerelatert innhold",
  "main": "package.json",
  "author": "NAV",
  "license": "MIT",
  "scripts": {
    "start": "sanity start",
    "test": "sanity check",
    "build": "sanity build",
    "docker": "npm run build && docker build -t dp-faktasider-cms . && docker run -p 8000:8000 dp-faktasider-cms",
    "backup": "sanity dataset export production production.tar.gz --overwrite",
    "clearDevelopmentDataset": "sanity dataset delete development && sanity dataset create development --visibility public",
    "copyProdDataToDevelopment": "npm run backup && npm run clearDevelopmentDataset && sanity dataset import production.tar.gz development --replace",
    "prepare": "husky install",
    "check-updates": "ncu -i --format group"
  },
  "prettier": {
    "printWidth": 120
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --list-different",
      "eslint --max-warnings 0"
    ]
  },
  "dependencies": {
    "@navikt/ds-css": "^0.18.14",
    "@navikt/ds-icons": "^0.8.17",
    "@navikt/ds-react": "^0.19.25",
    "@sanity/base": "^2.30.1",
    "@sanity/block-content-to-react": "^3.0.0",
    "@sanity/client": "^3.3.2",
    "@sanity/components": "^2.14.0",
    "@sanity/core": "^2.30.2",
    "@sanity/dashboard": "^2.30.1",
    "@sanity/default-layout": "^2.30.1",
    "@sanity/default-login": "^2.30.1",
    "@sanity/desk-tool": "^2.30.1",
    "@sanity/document-internationalization": "^0.3.2",
    "@sanity/image-url": "^1.0.1",
    "@sanity/vision": "^2.30.1",
    "@types/react": "^17.0.48",
    "nav-frontend-js-utils": "^1.0.20",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "styled-components": "^5.3.5",
    "swr": "^1.3.0",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@sanity/cli": "^2.30.2",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^14.1.1",
    "prettier": "^2.7.1"
  }
}
